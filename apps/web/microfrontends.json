{
	"$schema": "https://openapi.vercel.sh/microfrontends.json",
	"applications": {
		"web": {
			"packageName": "web",
			"development": {
				"fallback": "studyx-flax.vercel.app",
				"local": {
					"port": 3000
				}
			}
		},
		"studyx": {
			"packageName": "admin",
			"development": {
				"fallback": "studyx-flax.vercel.app",
				"local": {
					"port": 3001
				}
			},
			"routing": [
				{
					"group": "admin",
					"paths": ["/admin/:path*"]
				}
			]
		}
	}
}
